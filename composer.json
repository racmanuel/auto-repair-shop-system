{
    "name": "racmanuel/auto-repair-shop-system",
    "description": "My brilliant WordPress site",
    "repositories":[
        {
            "type":"composer",
            "url":"https://wpackagist.org",
            "only": [
                "wpackagist-plugin/*",
                "wpackagist-theme/*"
            ]
        }
    ],
    "extra": {
        "installer-paths": {
            "vendor/{$name}/": [
                "type:wordpress-plugin"
            ]
        }
    },
    "require": {
        "wpackagist-plugin/advanced-custom-fields": "^6.2"
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true
        }
    }
}
